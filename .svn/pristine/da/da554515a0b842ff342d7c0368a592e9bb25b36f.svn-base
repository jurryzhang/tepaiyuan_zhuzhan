/*首页接单按钮*/

.button {
  display: block;
  position: absolute;
  width: 2.08rem;
  height: 0.4rem;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
      -ms-transform: translate(-50%, -50%);
          transform: translate(-50%, -50%);
  background: #CCCCCC;
  border-radius: 0.4rem;
  cursor: pointer;
  -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
}
.button span {
  position: absolute;
  display: block;
}
.button span:first-of-type {
  z-index: 100;
  top: 0.03rem;
  right: 0.04rem;
  width: 1rem;
  height: 0.34rem;
  background: #000000;
  opacity: 0.4;
  border-radius: 0.4rem;
  -webkit-transition: right 400ms cubic-bezier(1, 0, 0, 1), box-shadow 400ms ease;
          transition: right 400ms cubic-bezier(1, 0, 0, 1), box-shadow 400ms ease;
}
.button span:nth-last-of-type(-n+2) {
  z-index: 10;
  top: 0.03rem;
  width: 1rem;
  height: 0.34rem;
  -webkit-transition: opacity 800ms ease 100ms;
          transition: opacity 800ms ease 100ms;
}
.button span:nth-last-of-type(-n+2):after {
  position: absolute;
  top: 0.1rem;
  line-height: 1;
  font-family: "微软雅黑";
  font-weight: 800;
  font-size: 0.16rem;
  color: white;
}
.button span:nth-of-type(2) {
  left: 0.04rem;
  background: #cf3829;
  border-top-left-radius: 0.4rem;
  border-bottom-left-radius: 0.4rem;
  opacity: 0.2;
}
.button span:nth-of-type(2):after {
  content: "停止接单";
  left: 0.15rem;
}
.button span:last-of-type {
  right: 0.04rem;
  background: #2ecc71;
  border-top-right-radius: 0.4rem;
  border-bottom-right-radius: 0.4rem;
  opacity: 1;
}
.button span:last-of-type:after {
  content: "开始接单";
  right: 0.15rem;
}
.button input[type="checkbox"] {
  display: none;
}
.button input[type="checkbox"]:checked ~ span:first-of-type {
  right: 1.04rem;
}
.button input[type="checkbox"]:checked ~ span:nth-of-type(2) {
  opacity: 1;
}
.button input[type="checkbox"]:checked ~ span:last-of-type {
  opacity: 0.2;
}


