/*重置样式*/

@charset "UTF-8";
* {
    margin: 0;
    padding: 0;
}

html {
    width: 100%;
    height: 100%;
}
body {
    width: 100%;
    height: 100%;
    font-family: "Microsoft YaHei", "Helvetica Neue", Helvetica, sans-serif;
    background: #ffffff;
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-flex-direction: column;
    -ms-flex-direction: column;
    flex-direction: column;
}
/*dialog框样式必须*/
body { color: #555; line-height: 1; font-size: 0.12rem; }
/*dialog框样式必须 end*/
img {
    border: none;
    vertical-align: bottom;
}

a {
    text-decoration: none;
    border: none;
}

ul li,ol li {
    list-style-type: none;
}

i {
    font-style: normal;
}

.clear {
    clear: both;
}

#header {
    height: 0.4rem;
    width: 100%;
    display: flex;
    background: #f8f8f8;
    position: fixed;
    top: 0;
    z-index: 10;
}

#header i {
    width: 0.24rem;
    height: 0.24rem;
    margin: 0.08rem;
    text-align: center;
    line-height: 0.24rem;
    position: absolute;
}

#header .title {
    flex: 1;
    font-size: 0.2rem;
    text-align: center;
    line-height: 0.4rem;
}
#header .shuom {
    font-size: 0.14rem;
    text-align: right;
    line-height: 0.4rem;
    color: #333333;
    margin-right: 0.05rem;
}

#main {
    -webkit-box-flex: 1;
    -webkit-flex: 1;
    -ms-flex: 1;
    flex: 1;
    font-size: 0.16rem;
    padding: 0.4rem 0 0.46rem;
}

.add {
    height: 0.43rem;
    width: 100%;
    font-size: 0.12rem;
    line-height: 0.43rem;
    border-top: 0.01rem solid #efefef;
    border-bottom: 0.05rem solid #efefef;
}

.add span {
    padding: 0.05rem;
    line-height: 0.28rem;
}

.add .diqu {
    width: 2.42rem;
    height: 0.24rem;
    border: 0;
}

.add .dizhi {
    /*width:2.42rem;
    height: 0.24rem;
    border: 0;*/
    /*float: left;*/
}

.dingdannav {
    width: 100%;
    font-size: 0.12rem;
    overflow: hidden;
    height: 0.32rem;
    line-height: 0.32rem;
}

.dingdannav li {
    float: left;
    width: 49%;
    text-align: center;
}

.dingdannav li:nth-child(1) {
    border-right: 0.02rem solid #d2d2d2;
}

.dingdanlist {
    width: 86%;
    height: 3.5rem;
    overflow: scroll;
    margin: 0 auto;
    position: relative;
}

.dingdanlist ul li {
    width: 100%;
    height: 0.76rem;
    background: #98b564;
    border-radius: 0.04rem;
    margin: 0.05rem 0;
}

.dingdanlist ul li p {
    width: 100%;
    text-align: center;
    line-height: 0.28rem;
    color: #FFFFFF;
}

.dingdanlist ul li p:nth-child(1) {
    padding-top: 0.1rem;
}

.dingdanlist ul li strong {
    font-size: 0.2rem;
    color: #fbe807;
}

.dingdanlist ul li i {
    color: red;
}

.dingdanlist .yuyue {
    width: 100%;
    height: auto;
    position: absolute;
    left: 0;
    top: 0;
    display: block;
    padding-bottom: 0.42rem;
}

.dingdanlist .jishi {
    width: 100%;
    height: 100%;
    position: absolute;
    left: 0;
    top: 0;
    display: none;
}

.active {
    color: #059d3d;
    border-bottom: 0.01rem solid #059d3d;
}

.active a {
    color: #059d3d!important;
}

#jiedan {
    height: 0.46rem;
    width: 100%;
    border-bottom: 0.01rem solid #9f97a2;
    font-size: 0.2rem;
    position: fixed;
    bottom: 0.42rem;
    background: #fff;
}

#jiedan .off {
    position: absolute;
    left: 50%;
    top: 0;
    width: 0.5rem;
    height: 0.5rem;
    display: block;
    border-radius: 50%;
    border: 0.03rem solid #c8c8c8;
    margin-left: -0.25rem;
    margin-top: -0.25rem;
    font-size: 0.18rem;
    text-align: center;
    padding: 0.02rem;
    background: #FFFFFF;
}

#jiedan .on {
    position: absolute;
    left: 50%;
    top: 0;
    width: 0.5rem;
    height: 0.5rem;
    display: block;
    border-radius: 50%;
    border: 0.03rem solid #c8c8c8;
    margin-left: -0.25rem;
    margin-top: -0.25rem;
    font-size: 0.18rem;
    text-align: center;
    padding: 0.02rem;
    background: #FFFFFF;
}

#footer {
    width: 100%;
    height: 0.42rem;
    position: fixed;
    bottom: 0;
    background: #f8f8f8;
    z-index: 10;
}

#footer .nav {
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    margin-top: 0.06rem;
}

#footer .nav li {
    font-size: 0.14rem;
    -webkit-box-flex: 1;
    -webkit-flex: 1;
    -ms-flex: 1;
    flex: 1;
}

#footer .nav li a {
    color: #929292;
    display: block;
    width: 100%;
    height: 100%;
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-flex-direction: column;
    -ms-flex-direction: column;
    flex-direction: column;
    justify-content: center;
    -webkit-box-align: center;
    -webkit-align-items: center;
    -ms-flex-align: center;
    align-items: center;
}

